<?xml version="1.0"?>
<FindBugsFilter>
<Match>
   <Class name="~.*(PoolThread|SimpleThreadPool|AbstractConsoleServer|ExternalProcessManager)" />
   <Bug pattern="UW_UNCOND_WAIT,WA_NOT_IN_LOOP,TLW_TWO_LOCK_WAIT,SWL_SLEEP_WITH_LOCK_HELD" />
</Match>
<Match>
   <Class name="~.*(ThroughputLimiter)" />
   <Bug pattern="RV_RETURN_VALUE_IGNORED" />
</Match>
</FindBugsFilter>
